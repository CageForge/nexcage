{
  "ignorePatterns": [
    { "pattern": "https://github.com/cageforge/nexcage/issues/72" },
    { "pattern": "https://github.com/cageforge/nexcage/issues/72#issuecomment-" },
    { "pattern": "https://pve.proxmox.com/wiki/Manual:_pct" },
    { "pattern": "https://github.com/cageforge/nexcage/compare/" },
    { "pattern": "^\.\./examples/" },
